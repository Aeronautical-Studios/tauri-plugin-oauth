{
    "build": {
        "distDir": "../public",
        "devPath": "../public"
    },
    "tauri": {
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "com.tauri.oauth",
            "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
            "resources": [],
            "externalBin": [],
            "copyright": "",
            "category": "DeveloperTool",
            "shortDescription": "",
            "longDescription": ""
        },
        "updater": {
            "active": false
        },
        "allowlist": {
            "all": true
        },
        "windows": [
            {
                "title": "app",
                "width": 800,
                "height": 600,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
        }
    }
}
